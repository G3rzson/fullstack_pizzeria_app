generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Pizza {
  id               String   @id @default(uuid())
  pizzaName        String
  pizzaDescription String
  pizzaPrice32     Float
  pizzaPrice45     Float
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
